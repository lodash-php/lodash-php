{
    "name": "lodash-php/lodash-php",
    "description": "A port of Lodash to PHP",
    "keywords": [
        "lodash",
        "utility",
        "utilities",
        "php-library",
        "utility-library",
        "php-utility"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Pierre du Plessis",
            "email": "open-source@solidworx.co"
        }
    ],
    "autoload": {
        "files": [
            "src/bootstrap.php"
        ]
    },
    "require": {
        "php": "^7.2 || ^8.0",
        "sebastian/comparator": "^1.2 | ^2.0 | ^2.1 | ^3.0 | ^4.0 | ^5.0",
        "symfony/property-access": "^2.7 | ^3.0 | ^4.0 | ^5.0 | ^6.0"
    },
    "require-dev": {
        "phpdocumentor/reflection-docblock": "*",
        "phpstan/phpdoc-parser": "*",
        "phpunit/phpunit": "^7.3 || ^8.4",
        "phpstan/phpstan": "^1.10",
        "friendsofphp/php-cs-fixer": "^2.12"
    }
}
